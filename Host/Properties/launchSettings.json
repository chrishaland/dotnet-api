{
  "profiles": {
    "Kestrel": {
      "commandName": "Project",
      "launchBrowser": false,
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "ASPNETCORE_APIPORT": "5000",
        "ASPNETCORE_MANAGEMENTPORT": "5001"
      },
